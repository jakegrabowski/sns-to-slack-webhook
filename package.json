{
  "name": "sns-to-slack-webhook",
  "version": "0.1.0",
  "keywords": ["awscdk", "aws-cdk", "cdk", "sns", "slack"],
  "author": "Jake Grabowski",
  "license": "MIT",
  "description": "AWS CDK construct that forwards Amazon SNS messages to Slack via Incoming Webhooks.",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakegrabowski/sns-to-slack.git"
    },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "lambda", "README.md", "LICENSE"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./lambda/*": "./lambda/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint **/*.ts",
    "go": "tsc && node ./dist/index.js",
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  },
  "dependencies": {
    "@slack/webhook": "^7.0.5",
    "axios": "^1.10.0"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "@types/node": "^20.0.0",
    "@types/aws-lambda": "^8.10.152",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0"
  }
}
